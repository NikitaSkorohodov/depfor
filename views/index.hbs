<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Navbar Example</title>
  <style>
    

body {
    margin: 0;
    padding-top: 106px; /* Высота навбара */
}

/* Стили для карусели */
.carousel-container {
    width: 100%;
    overflow: hidden;
}

.carousel {
    display: flex;
    overflow-x: hidden; /* Убрали скролл */
    scroll-snap-type: x mandatory;
    -webkit-overflow-scrolling: touch;
    white-space: nowrap; /* Добавлено для предотвращения переноса */
}

.carousel-slide {
    flex: 0 0 auto;
    scroll-snap-align: start;
    width: 100%;
}

button.prev, button.next {
    background: transparent; /* Прозрачный фон */
    border: none; /* Без границы */
    cursor: pointer; /* Указатель курсора */
    outline: none; /* Убираем обводку при клике */
    font-size: 2rem; /* Размер шрифта для стрелок */
    color: gray; /* Цвет стрелок */
}

button.prev:active, button.next:active {
    color: gray; /* Оставляем тот же цвет при клике */
    background: transparent; /* Прозрачный фон при клике */
}

button.prev:hover, button.next:hover {
    color: black; /* Легкий эффект при наведении */
}

.anchor-container {
    display: flex;
    justify-content: center; /* Центрирование по горизонтали */
    position: relative;
    margin-top: -100px; /* Поднимите якорь выше на 100px (настраивайте по необходимости) */
    margin-bottom: 20px; /* Отступ снизу (при необходимости) */
}

.anchor-container img {
    width: 70px; /* Увеличенный размер */
    height: 70px; /* Увеличенный размер */
}

/* Стили для карточек продуктов */
.cards-container {
    display: flex;
    flex-wrap: wrap;
    justify-content: space-around;
    margin: 20px 0;
}

.card {
    background: #fff;
    border-radius: 10px;
    box-shadow: 0 4px 8px rgba(0,0,0,0.1);
    overflow: hidden;
    width: 300px; /* Увеличенный размер карточек */
    margin: 10px;
    text-align: center;
    transition: transform 0.3s ease;
}

.card img {
    width: 100%;
    height: auto;
}

.card-content {
    padding: 15px;
}

.card-content h3 {
    margin: 10px 0;
    font-size: 22px; /* Увеличенный размер шрифта */
}

.card:hover {
    transform: scale(1.05);
}
.custom-heading {
    color: #ffeb3b; /* Желтый цвет текста */
    background-color: rgb(117, 2, 117); /* Фиолетовый фон */
    border: 5px solid rgb(117, 2, 117); /* Фиолетовая обводка */
    border-radius: 15px; /* Округлые края */
    padding: 10px; /* Внутренний отступ */
    display: inline-block; /* Чтобы обводка следовала за контентом */
    font-family: Arial, sans-serif; /* Шрифт текста */
    
    box-shadow: 4px 4px 15px rgba(0, 0, 0, 0.6); /* Тень вокруг заголовка */
  }

  </style>
</head>
<body>
  
  <h1 class="custom-heading">{{__ 'FEAUTURE WITH YOU!'}}</h1>

  <div class="carousel-container">
    <button class="prev" onclick="plusSlides(-1)">&#10094;</button>
    <div class="carousel">
        <div class="carousel-slide">
            <img src="https://images.1a.ee/display/onea_ee/uploads/fpe/1bde6491/1a9c542b.png?h=420&op=fit&w=970" alt="Slide 1">
        </div>
        <div class="carousel-slide">
            <img src="https://images.1a.ee/display/onea_ee/uploads/fpe/0524de57/955b4bf6.png?h=420&op=fit&w=970" alt="Slide 2">
        </div>
        <div class="carousel-slide">
            <img src="https://images.1a.ee/display/onea_ee/uploads/fpe/250444ec/c01fe3de.png?h=420&op=fit&w=970" alt="Slide 3">
        </div>
    </div>
    <button class="next" onclick="plusSlides(1)">&#10095;</button>
  </div>

  <script type="text/javascript">
    let slideIndex = 1;
    showSlides(slideIndex);

    function plusSlides(n) {
        showSlides(slideIndex += n);
    }

    function showSlides(n) {
        const slides = document.getElementsByClassName("carousel-slide");
        if (n > slides.length) {slideIndex = 1}
        if (n < 1) {slideIndex = slides.length}
        for (let i = 0; i < slides.length; i++) {
            slides[i].style.display = "none";
        }
        slides[slideIndex-1].style.display = "block";
    }

    // Add automatic scrolling every 3 seconds
    let slideInterval = setInterval(function() { plusSlides(1); }, 3000);

    // Stop automatic scrolling when hovering over the slider
    document.querySelector('.carousel-container').addEventListener('mouseenter', function() {
        clearInterval(slideInterval);
    });

    // Resume automatic scrolling when leaving the slider
    document.querySelector('.carousel-container').addEventListener('mouseleave', function() {
        slideInterval = setInterval(function() { plusSlides(1); }, 3000);
    });
  </script>

  <div class="anchor-container">
    <a href="#course-list">
        <img src="https://www.svgrepo.com/show/222080/down-arrow.svg" alt="Down arrow">
    </a>
  </div>

  <!-- Якорь, к которому будет переход -->
  <div id="course-list"></div>
  <h2>{{__ 'the newest technology!'}}</h2>

  <!-- Новый раздел с карточками продуктов -->
  <div class="cards-container">
    <div class="card">
        <a href="/products/search?q=apple">
            <img src="https://images.1a.ee/display/onea_ee/uploads/fpe/e91a82e6/2faf447b.png?op=fit" alt="Product 1">
            <div class="card-content">
                <h3>{{__ 'apple'}}</h3>
            </div>
        </a>
    </div>
    <div class="card">
        <a href="/products/search?q=intel">
            <img src="data:image/png;base64," alt="Product 2">
            <div class="card-content">
                <h3>{{__ 'intel'}}</h3>
            </div>
        </a>
    </div>
    <div class="card">
        <a href="/products/search?q=GeForce+">
            <img src="data:image/png;base64," alt="Product 3">
            <div class="card-content">
                <h3>{{__ 'nvidia'}}</h3>
            </div>
        </a>
    </div>
    <!-- Добавьте больше карточек по необходимости -->
  </div>

</body>
</html>